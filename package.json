{
  "name": "minesweeper-react-redux",
  "version": "1.0.0",
  "description": "Minesweeper React/Redux",
  "author": "Alexander Ishchenko",
  "homepage": "https://qialex.github.io/minesweeper-react-redux/",
  "keywords": [],
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "build": "webpack --mode production",
    "test": "jest",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest"
    },
    "setupFiles": [
      "<rootDir>test/_setup/enzyme.js",
      "<rootDir>test/_setup/localStorageMock.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/test/_setup/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/test/_setup/"
    ]
  },
  "engines": {
    "node": "=11.0.0",
    "npm": "=6.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qialex/minesweeper-react-redux.git"
  },
  "bugs": {
    "url": "https://github.com/qialex/minesweeper-react-redux/issues"
  },
  "homepage": "https://github.com/qialex/minesweeper-react-redux",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "gh-pages": "^2.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.7",
    "jest": "^23.6.0",
    "node-sass": "^4.11.0",
    "react": "^16.3.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.3.0",
    "react-localization": "^1.0.13",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.7.0",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "sinon": "^7.2.2",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.2.0",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {}
}
